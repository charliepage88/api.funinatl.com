{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "algolia/algoliasearch-client-php": "^2.2",
        "babenkoivan/scout-elasticsearch-driver": "^3.11",
        "barryvdh/laravel-cors": "^0.11.3",
        "doctrine/dbal": "^2.9",
        "fabpot/goutte": "^3.2",
        "fideloper/proxy": "^4.0",
        "geocoder-php/algolia-places-provider": "^0.1.0",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/mongodb": "^3.5",
        "johngrogg/ics-parser": "^2.1",
        "jwilsson/spotify-web-api-php": "^2.7",
        "laravel/framework": "5.8.*",
        "laravel/scout": "^7.1",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/message": "^1.7",
        "predis/predis": "^1.1",
        "silber/bouncer": "v1.0.0-rc.5",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-newsletter": "^4.5",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-tags": "^2.2",
        "symfony/dom-crawler": "^4.3",
        "willdurand/geocoder": "^4.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
