{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "appstract/laravel-meta": "^2.0",
        "babenkoivan/scout-elasticsearch-driver": "^4.0.3",
        "fruitcake/laravel-cors": "^1.0",
        "doctrine/dbal": "^2.9",
        "fabpot/goutte": "^4.0",
        "fideloper/proxy": "^4.2",
        "geocoder-php/algolia-places-provider": "^0.1.0",
        "guzzlehttp/guzzle": "^6.3",
        "igorw/get-in": "^1.0",
        "johngrogg/ics-parser": "^2.1",
        "jwilsson/spotify-web-api-php": "^3.1",
        "laravel/framework": "^7.0",
        "laravel/passport": "^8.4",
        "laravel/scout": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "php-http/message": "^1.8",
        "predis/predis": "^1.1",
        "thujohn/twitter": "^2.2",
        "silber/bouncer": "v1.0.0-rc.7",
        "spatie/laravel-medialibrary": "^8.0",
        "spatie/laravel-newsletter": "^4.6",
        "spatie/laravel-responsecache": "^6.3",
        "spatie/laravel-sluggable": "^2.2",
        "spatie/laravel-tags": "^2.5",
        "symfony/dom-crawler": "^5.0",
        "willdurand/geocoder": "^4.2"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
