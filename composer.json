{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "appstract/laravel-meta": "^2.0",
        "babenkoivan/scout-elasticsearch-driver": "^4.3",
        "fruitcake/laravel-cors": "^2.0",
        "doctrine/dbal": "^3.0",
        "fabpot/goutte": "^4.0",
        "fideloper/proxy": "^4.4",
        "geocoder-php/algolia-places-provider": "^0.3",
        "guzzlehttp/guzzle": "^7.3",
        "igorw/get-in": "^1.0",
        "johngrogg/ics-parser": "^2.2",
        "jwilsson/spotify-web-api-php": "^4.3",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.1",
        "laravel/scout": "^8.6",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.2",
        "league/flysystem-aws-s3-v3": "^1.0",
        "php-http/message": "^1.11",
        "predis/predis": "^1.1",
        "thujohn/twitter": "^3.0",
        "silber/bouncer": "v1.0.0-rc.10",
        "spatie/laravel-medialibrary": "^9.5",
        "spatie/laravel-newsletter": "^4.9",
        "spatie/laravel-responsecache": "^6.3",
        "spatie/laravel-sluggable": "^2.6",
        "spatie/laravel-tags": "^3.1",
        "symfony/dom-crawler": "^5.2",
        "willdurand/geocoder": "^4.4"
    },
    "require-dev": {
        "facade/ignition": "^2.8",
        "fakerphp/faker": "^1.1",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.4",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
